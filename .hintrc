{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/aria": "off",
    "axe/parsing": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "background-clip",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "scroll-behavior",
          "-webkit-overflow-scrolling",
          "text-decoration-skip-ink",
          "-webkit-tap-highlight-color"
        ]
      }
    ]
  }
}